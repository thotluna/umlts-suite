{
  "name": "@umlts/plugin-ts",
  "version": "0.1.0",
  "description": "TypeScript language support for UMLTS",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --sourcemap",
    "dev": "tsup src/index.ts --format cjs,esm --dts --sourcemap --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "uml",
    "typescript",
    "plugin",
    "umlts"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@umlts/engine": "workspace:*"
  },
  "devDependencies": {
    "@umlts/engine": "workspace:*",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
